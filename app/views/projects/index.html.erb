
<h1>Projects</h1>

<ul>
  <% @projects.each do |project| %>
    <li>
        <%= link_to project.name, project_path(project) %>
        <%= link_to 'Edit', edit_project_path(project) %>
        <li><%= button_to "Destroy", project_path(project), method: :delete, data: { confirm: "Are you sure?" } %></li>
    </li>
  <% end %>
</ul>


<h1>Developers</h1>

<ul>
  <% @developers.each do |developer| %>
    <li>
        <%= link_to developer.user_name, developer_path(developer) %>
        <%= link_to 'Edit', edit_developer_path(developer) %>
        <li><%= button_to "Destroy", developer_path(developer), method: :delete, data: { confirm: "Are you sure?" } %></li>
    </li>
  <% end %>
</ul>


<%= link_to 'Add Developers', new_developer_path%>